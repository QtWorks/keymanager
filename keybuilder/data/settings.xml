      <Parameter name="TOLERANCE" type="double" variable="qt_tolerance_qt" default="0.1" ui="lineedit" />
      <Parameter name="SHOW CUTS" type="string" variable="qt_show_chosen_cuts_qt" default="NO" ui="yesno" />
   </Block>
</Block>
