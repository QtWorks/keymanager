module abloy_disklock_anglebase(degres) 
{
	rotate([0,degres,0])
	translate ([-5,0,-2-(4.5/2)])
	cube([10,abloy_disklock_mill_thickness,2], false);

	rotate([0,180,0])
	rotate([0,degres,0])
	translate ([-5,0,-2-(4.5/2)])
	cube([10,abloy_disklock_mill_thickness,2], false);
}
module abloy_disklock_angle0() {}
