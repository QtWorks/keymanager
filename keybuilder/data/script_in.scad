	rotate ([-90,0,0])
	difference() 
	{
		cylinder (h=abloy_disklock_mill_thickness, d=10, $fn=100) ;
		translate ([0,0,-1]) 
			cylinder (h=4, d=4.5, $fn=100) ;
	}
}

